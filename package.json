{
  "name": "contact-management-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "npx tsc",
    "dev": "npx nodemon --watch src --ext ts --exec \"npx tsc && node dist/index.js\"",
    "ts-node": "npx ts-node --esm src/index.ts",
    "tsc-watch": "npx tsc -w",
    "seed": "npx ts-node src/db/seedDatabase.ts"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^15.12.2",
    "@types/sqlite3": "^3.1.11",
    "@types/uuid": "^8.3.4",
    "nodemon": "^2.0.12",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  }
}
